//Special NSLog Macro that only runs in DEBUG mode
#ifdef DEBUG
#define DLog(fmt, ...) NSLog((@"%@: " fmt), NSStringFromClass([self class]), ##__VA_ARGS__);
#else
#define DLog(...)
#endif